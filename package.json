{
  "name": "@sanchezjjose/react-toggle-component",
  "description": "A react toggle component.",
  "author": "Jose Sanchez <sanchezjjose@gmail.com>",
  "version": "0.1.4",
  "main": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sanchezjjose/react-toggle-component"
  },
  "bugs": {
    "url": "https://github.com/sanchezjjose/react-toggle-component/issues"
  },
  "keywords": [
    "react-component",
    "react",
    "toggle"
  ],
  "files": [
    "./dist/npm/components"
  ],
  "directories": {
    "lib": "./src",
    "example": "./example"
  },
  "scripts": {
    "build": "browserify src/index.js -t reactify -d",
    "build:dev": "npm --loglevel=silent run build > dist/react-toggle-component.dev.js",
    "build:min": "npm --loglevel=silent run build -- | uglifyjs -cmvb beautify=false,ascii_only=true - > dist/react-toggle-component.min.js",
    "build:example": "browserify example/index.js -s ReactToggleComponent -t reactify -d > example/react-toggle-component.example.js",
    "build:all": "npm run build:dev && npm run build:min && npm run build:example",
    "prepublish": "npm run jsx && npm run lint",
    "lint": "jshint dist/npm/components/*.js",
    "jsx": "jsx ./src/components ./dist/npm/components -x jsx"
  },
  "dependencies": {
    "jquery": "~2.1.4",
    "node-lessify": "0.0.10",
    "react": "^0.14.1",
    "react-dom": "^0.14.1"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "jshint": "^2.7.0",
    "react-tools": "^0.13.2",
    "reactify": "^1.1.0",
    "uglify-js": "^2.4.20"
  }
}
